{
    "name" : "local",
    "oauth_endpoints" : {
        "token" : {
            "preserve_host" : false,
            "listen_path" : "/auth/token",
            "upstream_url" : "http://localhost:9088/token",
            "strip_path" : true,
            "append_path" : false,
            "enable_load_balancing" : false,
            "methods" : [
                "GET", 
                "POST"
            ]
        }
    },
    "token_strategy" : {
        "name" : "jwt",
        "settings" : {
            "secret" : "secret"
        }
    }
}
